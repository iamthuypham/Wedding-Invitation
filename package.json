{
  "name": "my-wedding-invitation-2018",
  "version": "0.1.0",
  "description": "My wedding invitation app",
  "engines": {
    "node": "6.2.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "web-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
    "web-dev": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public/ --config webpack.config.js --port 8080 --inline --hot --colors",
    "web-dev-c9": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public/ --config webpack.config.js --port $PORT --host $IP --inline --hot --colors",
    "postinstall": "webpack -p"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "ejs": "2.5.6",
    "express": "4.15.2",
    "material-ui": "^1.0.0-beta.20",
    "react": "^15.6.1",
    "react-anime": "^2.0.1",
    "react-countdown-now": "^1.2.0",
    "react-dom": "^15.6.1",
    "react-google-maps": "^7.1.0",
    "react-loading": "^1.0.1",
    "react-router": "^4.2.0",
    "react-scrollable-anchor": "^0.5.0",
    "react-sound": "^0.8.0",
    "react-spinkit": "^3.0.0",
    "react-transition-group": "^1.2.1",
    "react-typist": "^2.0.4",
    "webpack": "^3.5.6",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "react-countdown-now": "^1.2.0",
    "request": "^2.81.0",
    "style-loader": "^0.19.0",
    "tape": "^4.7.0",
    "webpack-dev-server": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremyhatter/asylum-connect-1deg-catalog"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
